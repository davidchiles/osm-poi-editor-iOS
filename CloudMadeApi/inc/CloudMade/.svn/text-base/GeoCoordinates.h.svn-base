#import <Foundation/Foundation.h>

//! Class presents geocoordinates latitude and longitude
@interface GeoCoordinates : NSObject
{
	float fLatitude; /**< latitude */
	float fLongitude;/**< longitude */
}
//! latitude
@property (nonatomic,readwrite) float fLatitude;
//! longitude
@property (nonatomic,readwrite) float fLongitude;
/**
 *  Creates object
 *  @param lat latitude
 *  @param longitude
 */
+(id) withLat:(float)lat Lng:(float)lng;
/**
 *  Inits object
 *  @param lat latitude
 *  @param longitude
 */
-(id) initWithCoordinates:(float) lat :(float) lng;
@end

